<lockup
  data-href="templates/ItemDetail.tvml"
  data-media-id="${this.mediaid}"
  data-related-playlist="${this.related}"
  data-parent-view="${this.parentView}"
  data-view="ListItem">
  <img class="item-image" src="${this.artworkImageURL}" />
  <title style="tv-text-style: none; font-size:24; tv-text-max-lines: 2" class="scrollTextOnHighlight">${he.strip(this.title)}</title>
</lockup>
